  {
    "key": "Tab",
    "command": "editor.toggleFold",
    "when": "editorTextFocus && foldingEnabled && editorLangId == 'org'"
  },
  {
    "key": "ctrl+enter",
    "command": "org.insertHeadingRespectContent",
    "when": "editorLangId == 'org'"
